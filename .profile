export PATH=~/.local/bin:~/.config/yarn/global/node_modules/.bin:~/.cargo/bin:$GOPATH/bin:/sbin:/usr/sbin:/usr/games/bin:$PATH
export VISUAL=$EDITOR
export ZDOTDIR=~/.zsh.d
